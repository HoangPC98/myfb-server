{
    "name": "fb-server",
    "version": "0.0.1",
    "description": "",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "prebuild": "rimraf dist",
        "copy-assets": "mkdir -p dist/assets && yes | cp -rf src/views/ dist/assets/",
        "build": "npm run copy-assets && nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "npm run build && nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint:check": "eslint \"{src,apps,libs,test}/**/*.ts\"",
        "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "seed:config": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js config -n src/database/typeormconfig.ts",
        "seed:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js seed -n src/database/typeormconfig.ts",
        "schema:drop": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -f src/database/typeormconfig.ts schema:drop",
        "schema:sync": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -f src/database/typeormconfig.ts schema:sync",
        "mig:gen": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -f src/database/typeormconfig.ts migration:generate -n",
        "mig:create": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -f src/database/typeormconfig.ts migration:create -n",
        "mig:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -f src/database/typeormconfig.ts migration:run -t=false",
        "mig:revert": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -f src/database/typeormconfig.ts migration:revert"
    },
    "dependencies": {
        "@casl/ability": "^5.4.3",
        "@nestjs/common": "^8.0.0",
        "@nestjs/config": "^1.1.5",
        "@nestjs/core": "^8.0.0",
        "@nestjs/jwt": "^8.0.0",
        "@nestjs/mapped-types": "*",
        "@nestjs/passport": "^8.0.1",
        "@nestjs/platform-express": "^8.0.0",
        "@nestjs/schedule": "^1.1.0",
        "@nestjs/serve-static": "^2.2.2",
        "@nestjs/swagger": "^5.1.5",
        "@nestjs/typeorm": "^8.0.2",
        "@types/passport-jwt": "^3.0.6",
        "@types/pdfmake": "^0.1.20",
        "@types/randomcolor": "^0.5.6",
        "argon2": "^0.28.3",
        "bcrypt": "^5.1.0",
        "cache-manager": "^3.6.3",
        "cache-manager-redis-store": "^2.0.0",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.13.2",
        "dayjs": "^1.11.0",
        "ejs": "^3.1.8",
        "exceljs": "^4.3.0",
        "fast-csv": "^4.3.6",
        "firebase": "^9.6.7",
        "firebase-admin": "^10.0.2",
        "google-auth-library": "^7.11.0",
        "ioredis": "^5.2.4",
        "multer": "^1.4.4",
        "mysql2": "^2.3.3",
        "nodemailer": "^6.8.0",
        "otplib": "^12.0.1",
        "passport": "^0.6.0",
        "passport-google-oauth20": "^2.0.0",
        "passport-jwt": "^4.0.0",
        "pdfmake": "^0.2.4",
        "randomcolor": "^0.6.2",
        "redis-cli": "^1.4.0",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.2",
        "rxjs": "^7.2.0",
        "stream-to-array": "^2.3.0",
        "swagger-ui-express": "^4.2.0",
        "tmp": "^0.2.1",
        "ts-node-dev": "^2.0.0",
        "twilio": "^3.83.4",
        "typeorm": "^0.2.41",
        "uuid": "^8.3.2",
        "xlsx": "^0.17.4"
    },
    "devDependencies": {
        "@nestjs/cli": "^8.0.0",
        "@nestjs/schematics": "^8.0.0",
        "@nestjs/testing": "^8.0.0",
        "@types/cache-manager": "^3.4.3",
        "@types/cache-manager-redis-store": "^2.0.1",
        "@types/cron": "^1.7.3",
        "@types/express": "^4.17.13",
        "@types/jest": "27.0.2",
        "@types/multer": "^1.4.7",
        "@types/node": "^16.0.0",
        "@types/passport-google-oauth20": "^2.0.11",
        "@types/supertest": "^2.0.11",
        "@types/uuid": "^8.3.4",
        "@typescript-eslint/eslint-plugin": "^5.0.0",
        "@typescript-eslint/parser": "^5.0.0",
        "eslint": "^8.0.1",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "jest": "^27.2.5",
        "prettier": "^2.3.2",
        "source-map-support": "^0.5.20",
        "supertest": "^6.1.3",
        "ts-jest": "^27.0.3",
        "ts-loader": "^9.2.3",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^3.10.1",
        "typeorm-seeding": "^1.6.1",
        "typescript": "^4.3.5"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
